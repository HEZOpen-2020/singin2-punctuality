    "version": "0.2.0",
    "configurations": [
        {
            "name": "快速编译运行：调试任务",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}\\.vscode\\debug-cpp\\cdebug.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": true,
            "preLaunchTask": "dbg_fake_g++",
            "MIMode": "gdb",
            "miDebuggerPath": "D:\\MinGW\\bin\\gdb.exe"
            // "setupCommands": [
            //     {
            //         "description": "Enable pretty-printing for gdb",
            //         "text": "-enable-pretty-printing",
            //         "ignoreFailures": true
            //     }
            // ]
        },
        {
            "name": "快速编译运行：常规任务",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}\\.vscode\\consolePauser.exe",
            "args": ["Run C++ single file","${fileDirname}","${workspaceRoot}\\.vscode\\debug-cpp\\cdebug.exe"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal",
            "preLaunchTask": "dbg_fake_g++"
        },
        {
            "name": "快速编译运行：校验器程序",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}\\.vscode\\consolePauser.exe",
            "args": [
                "Run C++ single file",
                "${fileDirname}",
                "${workspaceRoot}\\.vscode\\debug-cpp\\cdebug.exe",
                "in.txt",
                "out.txt",
                "std.txt",
                "100",
                "score.txt",
                "report.txt"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal",
            "preLaunchTask": "dbg_fake_g++"
        },
        {
            "name": "快速编译运行：只生成",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}\\.vscode\\consolePauser.exe",
            "args": [""],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "internalConsole",
            "preLaunchTask": "?dbg_fake_g++"
        },
        {
            "name": "编译运行：前置任务",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}\\.vscode\\fake_g++.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "internalConsole",
            "preLaunchTask": "fake_g++"
        },
        {
            "name": "编译运行：启动",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}\\.vscode\\fake_g++.exe",
            "args": ["${file}","${workspaceRoot}\\.vscode\\\\","${fileDirname}"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}\\.vscode",
            "environment": [],
            "console": "externalTerminal"
        },
        {
            "name": "快速编译运行：前置任务",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}\\.vscode\\fake_g++.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "internalConsole",
            "preLaunchTask": "!fake_g++"
        },
        {
            "name": "快速编译运行：启动",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}\\.vscode\\fake_g++.exe",
            "args": ["!${file}","${workspaceRoot}\\.vscode\\\\","${fileDirname}"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}\\.vscode",
            "environment": [],
            "console": "externalTerminal"
        }
    ]
}
